[
  {
    "description": "这是一个会返回cookie信息的get请求",
    "request": {
      "uri":"/getCookies",
      "method": "get"
    },
    "response": {
      "cookies": {
        "login": "true"
      },
      "text": "恭喜你获得cookies信息成功"
    }
  },

  {
    "description": "这是一个带cookie信息的get请求",
    "request": {
      "uri": "/testGetCookies",
      "method": "get",
      "cookies":{
        "login":"true"
      }
    },
    "response": {
      "text": "这是一个需要携带cookie信息才能访问的get请求"
    }
  },
  {
    "description": "这是一个带cookie信息的post请求",
    "request": {
      "uri": "/post/with/cookie",
      "method": "post",
      "cookies": {
        "login": "true"
      },
      "json": {
        "name": "huhansan",
        "age": "35"
      }
    },
    "response": {
      "status": "200",
      "json": {
        "huhansan": "success",
        "status": "1"
      }
    }
  }
]